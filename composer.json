{
	"name": "jweiland/circular",
	"description": "Organize your circular records incl. PDF download",
	"license": "GPL-2.0-or-later",
	"type": "typo3-cms-extension",
	"keywords": [
		"typo3",
		"TYPO3 CMS",
		"circular",
		"circular"
	],
	"authors": [
		{
			"name": "Stefan Froemken",
			"email": "sfroemken@jweiland.net",
			"role": "Lead Developer"
		},
		{
			"name": "Hoja Mustaffa Abdul Latheef",
			"email": "hlatheef@jweiland.net",
			"role": "Senior Developer"
		}
	],
	"homepage": "https://jweiland.net",
	"support": {
		"email": "projects@jweiland.net",
		"issues": "https://github.com/jweiland-net/circular/issues",
		"source": "https://github.com/jweiland-net/circular"
	},
	"require": {
		"typo3/cms-core": "^12.4"
	},
	"require-dev": {
		"ergebnis/composer-normalize": "~2.42.0",
		"friendsofphp/php-cs-fixer": "^3.14",
		"phpunit/phpunit": "^9.6",
		"typo3/coding-standards": "^0.6",
		"typo3/testing-framework": "^7.0.2"
	},
	"replace": {
		"typo3-ter/circular": "self.version"
	},
	"autoload": {
		"psr-4": {
			"JWeiland\\Circular\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"JWeiland\\Circular\\Tests\\": "Tests"
		}
	},
	"config": {
		"allow-plugins": {
			"ergebnis/composer-normalize": true,
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		},
		"bin-dir": ".Build/bin",
		"vendor-dir": ".Build/vendor"
	},
	"extra": {
		"typo3/cms": {
			"app-dir": ".Build",
			"extension-key": "circular",
			"web-dir": ".Build/public"
		}
	}
}
